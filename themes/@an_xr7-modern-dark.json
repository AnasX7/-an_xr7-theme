  "tokenColors": [
    {
      "name": "Global settings",
      "settings": {
        "background": "#323545",
        "foreground": "#B4B8CF"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#BCDE8C"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation, constant.other.symbol",
      "settings": {
        "foreground": "#88D4F2"
      }
    },
    {
      "name": "String Escape",
      "scope": "constant.character.escape, text.html constant.character.entity.named",
      "settings": {
        "foreground": "#B4B8CF"
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#F299A8"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#EB8B6E"
      }
    },
    {
      "name": "Variable",
      "scope": "variable, variable.parameter, support.variable, variable.language, support.constant, meta.definition.variable entity.name.function, meta.function-call.arguments",
      "settings": {
        "foreground": "#B4B8CF"
      }
    },
    {
      "name": "Other Keyword",
      "scope": "keyword.other",
      "settings": {
        "foreground": "#EB8B6E"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword, modifier, variable.language.this, support.type.object, constant.language",
      "settings": {
        "foreground": "#88D4F2"
      }
    },
    {
      "name": "Function call",
      "scope": "entity.name.function, support.function",
      "settings": {
        "foreground": "#82A6F2"
      }
    },
    {
      "name": "Storage",
      "scope": "storage.type, storage.modifier, storage.control",
      "settings": {
        "foreground": "#C090DF"
      }
    },
    {
      "name": "Modules",
      "scope": "support.module, support.node",
      "settings": {
        "foreground": "#F299A8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type",
      "scope": "support.type, constant.other.key",
      "settings": {
        "foreground": "#F2C36D"
      }
    },
    {
      "name": "Type",
      "scope": "entity.name.type, entity.other.inherited-class, entity.other",
      "settings": {
        "foreground": "#F2C36D"
      }
